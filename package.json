{
  "name": "@nosto/web-components",
  "private": false,
  "author": {
    "name": "Nosto Solutions Oy"
  },
  "version": "7.5.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "main.cjs.js",
  "module": "main.es.js",
  "exports": {
    ".": {
      "import": "./dist/main.es.js",
      "browser": "./dist/main.es.bundle.js",
      "require": "./dist/main.cjs.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nosto/web-components.git"
  },
  "scripts": {
    "build": "tsc && node esbuild.mjs && npm run typedoc",
    "dev": "vite dev",
    "preview": "vite preview",
    "lint": "eslint",
    "lint-fix": "eslint --fix",
    "test": "vitest run --silent --coverage",
    "typedoc": "typedoc src/main.ts",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@nosto/nosto-js": "^2.3.0",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/express": "^5.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.8.7",
    "@types/serve-favicon": "^2.5.7",
    "@vitest/coverage-v8": "^3.0.5",
    "esbuild": "^0.25.1",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-barrel-files": "^2.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "express": "^4.21.2",
    "handlebars": "^4.7.8",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "liquidjs": "^10.20.2",
    "prettier": "^3.3.3",
    "serve-favicon": "^2.5.0",
    "swiper": "^11.2.6",
    "typedoc": "^0.27.9",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.12.2",
    "vite": "^6.3.4",
    "vitest": "^3.0.5"
  },
  "peerDependencies": {
    "handlebars": "4.x",
    "liquidjs": "10.x",
    "swiper": "11.x"
  },
  "peerDependenciesMeta": {
    "handlebars": {
      "optional": true
    },
    "liquidjs": {
      "optional": true
    },
    "swiper": {
      "optional": true
    }
  },
  "engines": {
    "node": ">= 22.12.0",
    "npm": ">= 10.9.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@unpic/core": "^1.0.1",
    "unpic": "^4.1.2"
  }
}
