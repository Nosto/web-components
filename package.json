{
  "name": "@nosto/web-components",
  "private": false,
  "author": {
    "name": "Nosto Solutions Oy"
  },
  "version": "8.18.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "main.cjs.js",
  "module": "main.es.js",
  "exports": {
    ".": {
      "import": "./dist/main.es.js",
      "browser": "./dist/main.es.bundle.js",
      "require": "./dist/main.cjs.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nosto/web-components.git"
  },
  "scripts": {
    "build": "tsc && node esbuild.mjs && npm run typedoc",
    "preview": "vite preview",
    "lint": "eslint",
    "lint-fix": "eslint --fix",
    "test": "vitest run --silent --coverage",
    "typecheck": "tsc --noEmit",
    "typedoc": "typedoc src/main.ts",
    "prepare": "husky",
    "visualize": "esbuild-visualizer --metadata ./meta.json",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@nosto/nosto-js": "^2.6.1",
    "@storybook/addon-docs": "^9.1.3",
    "@storybook/react-vite": "^9.1.3",
    "@storybook/web-components-vite": "^9.1.3",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.8.7",
    "@types/serve-favicon": "^2.5.7",
    "@unpic/core": "^1.0.1",
    "@vitest/coverage-v8": "^3.0.5",
    "esbuild": "^0.25.1",
    "esbuild-visualizer": "^0.7.0",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-barrel-files": "^2.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "lit": "^3.3.1",
    "prettier": "^3.3.3",
    "serve-favicon": "^2.5.0",
    "storybook": "^9.1.3",
    "swiper": "^11.2.6",
    "typedoc": "^0.27.9",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.12.2",
    "unpic": "^4.1.2",
    "vite": "^6.3.4",
    "vitest": "^3.0.5",
    "msw": "^2.10.5"
  },
  "engines": {
    "node": ">= 22.12.0",
    "npm": ">= 10.9.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
